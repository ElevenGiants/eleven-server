    "lynx": "^0.2.0",
    "multitransport-jsonrpc": "^0.7.2",
    "murmurhash-js": "^1.0.0",
    "nconf": "^0.6.9",
    "node_amf_cc": "^1.3.0",
    "rethinkdb": "^1.15.0-0",
    "segfault-handler": "^1.0.0",
    "slack-client": "^1.2.2",
    "slack-node": "^0.1.3",
